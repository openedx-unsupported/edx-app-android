    <string name="edit_user_profile_change_photo">Alterar</string>
    <!-- Content Description for change photo button -->
    <string name="edit_user_profile_change_photo_description">Altere a foto do perfil</string>
    <!-- Button that will open a camera for taking a new profile photo -->
    <string name="edit_user_profile_take_photo">Tirar foto</string>
    <!-- Button that will open photo gallery for choosing a new profile photo -->
    <string name="edit_user_profile_choose_photo">Escolher foto</string>
    <!-- Button that will open a camera for taking a new profile photo -->
    <string name="edit_user_profile_remove_photo">Remover foto</string>
