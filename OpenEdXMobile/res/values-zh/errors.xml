<?xml version='1.0' encoding='UTF-8'?>
<resources>
  <!--An error occurred but we don't know why-->
  <string name="error_unknown">未知错误。请稍后重试。</string>
  <!--Course related error messages-->
  <!--Error shown when a course's content is invalid or not in the format that we expect-->
  <string name="course_error_content_invalid">无法加载课程内容，请稍后重试。</string>
  <!--Title of alert shown when course enrollment request fails-->
  <string name="enrollment_error_title">选课出错</string>
  <!--Error message shown in alert when course enrollment request fails-->
  <string name="enrollment_error_message">您无法通过{platform_name}客户端进行选课。请使用网页浏览器重试。</string>
  <!--Error message shown when a course section is not available on mobile-->
  <string name="not_available_on_mobile">不支持移动端</string>
  <!--Network error messages-->
  <!--Error shown when  user attempts an action but needs an internet connection-->
  <string name="network_not_connected">未连接到网络</string>
  <!--Concise error shown when user attempts an action but needs an internet connection-->
  <string name="network_not_connected_short">网络无法连接</string>
  <!--Alert dialog title shown when a network request fails-->
  <string name="reset_no_network_title">连接错误</string>
  <!--Alert dialog content shown when a network request fails because the user has no internet connection-->
  <string name="reset_no_network_message">您没有连接到网络，请检查您的网络连接。</string>
  <!--An error ocurred due to some kind of network failure-->
  <string name="network_connected_error">连接错误。请检查您的网络连接。</string>
  <!--The server we are trying to use is not available right now-->
  <string name="network_service_unavailable">服务不可用。请稍后重试。</string>
  <!--Login error messages-->
  <!--Error shown during login when user attempts login without entering a username or e-mail-->
  <string name="error_enter_email">请输入您的用户名或邮箱，并重试。</string>
  <!--Error shown during login when user enters an invalid email address-->
  <string name="error_invalid_email">请确保您的邮箱格式正确，并重试。</string>
  <!--Error shown during login when user attempts login without entering a password-->
  <string name="error_enter_password">请输入您的密码重试。</string>
  <!--Title of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_title_fb">Facebook账号与{platform_name}账号不匹配</string>
  <!--Content of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_desc_fb">请确保您的Facebook账号与您{platform_destination}上的{platform_name}账号匹配。</string>
  <!--Content of alert shown during login when user attempts login via Facebook without having linked their account-->
  <string name="error_account_not_linked_desc_fb_2">此Facebook账号尚未与任何{platform_name}账号相关联，请先注册。</string>
  <!--Title of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_title_google">Google账号与{platform_name}账号未关联</string>
  <!--Content of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_desc_google">请确保您的Google账号与您{platform_destination}上的{platform_name}账号匹配。</string>
  <!--Content of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_desc_google_2">此Google账号尚未与任何{platform_name}账号相关联，请先注册。</string>
  <!--Title of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_title_microsoft">未与{platform_name}帐户关联的微软帐户</string>
  <!--Content of alert shown during login when user attempts login via Google without having linked their account-->
  <string name="error_account_not_linked_desc_microsoft">请确保您的微软账号与您{platform_destination}上的{platform_name}账号匹配。</string>
  <!--Content of alert shown during login when user attempts login via Microsoft without having linked their account-->
  <string name="error_account_not_linked_desc_microsoft_2">此微软账号尚未与任何{platform_name}账号相关联，请先注册。</string>
  <!--New version available-->
  <!--Snackbar notification shown when the current version of the app is unsupported-->
  <string name="app_version_unsupported">不支持您的新版本</string>
  <!--Snackbar notification shown when the current version of the app is deprecated-->
  <!--Snackbar notification shown when the current version of the app is outdated-->
  <string name="app_version_outdated">应用程序有新版本可用。</string>
  <!--Toast notification for when the user presses the update action button from the Snackbar
         notifications while no supported app store or browser is installed on the device with which
         the app could be updated-->
  <string name="app_version_upgrade_app_store_unavailable">在设备中未找到支持的应用商店</string>
  <!--Registration Error Messages-->
  <!--Error prompt shown during registration to indicate an empty field. Argument is a field name-->
  <string name="error_enter_field">请输入您的%1$s。</string>
  <!--Error prompt shown during registration to indicate a field invalid input. Argument is a field name-->
  <string name="error_select_or_enter_field">请选择或输入一个有效的%1$s。</string>
  <!--Error prompt shown during registration when a field entry is too short. Arguments are 1) a field name, 2) a number-->
  <string name="error_min_length">%1$s 至少包含 %2$d 个字符。</string>
  <!--Error prompt shown during registration when a field entry is too long. Arguments are 1) a field name, 2) a number-->
  <string name="error_max_length">%1$s 不能超过 %2$d 个字符。</string>
  <!--Error message shown when a course hasn't started yet-->
  <string name="course_not_started">本课程尚未开始。</string>
  <!--Error message shown when a user's access has expired for a course-->
  <string name="course_access_expired">您的访问权限已过期。</string>
  <!--Error message shown when a user can't perform an action on e.g. add a post/response/comment in forums-->
  <string name="action_not_completed">您的操作无法完成。</string>
  <!--Error dialog message shown when the user try to log in in app with un-supported version-->
  <string name="app_version_unsupported_login_msg">您的应用版本不再受支持。请更新到最新版本后再登录。</string>
  <!--Error dialog message shown when the user try to register in app with un-supported version-->
  <string name="app_version_unsupported_register_msg">您的应用版本不再受支持。请更新到最新版本后再注册。</string>
  <!--Permission Error Messages-->
  <!--Error message shown when a permission is not granted-->
  <string name="permission_not_granted">未授予许可</string>
  <!--This error message will be shown if user tried to login with third party (facebook/google) and server returns 403 status code, Disabled user-->
  <string name="auth_provider_disabled_user_error">您的账户已禁用。请联系客户支持寻求帮助。</string>
</resources>
