    <string name="settings_wifi_bottom">Nur herunterladen, wenn WiFi aktiviert ist</string>
    <!-- Text to clarify that around what is visible -->
    <string name="profile_visibility_off_message">Ihre Profilinformationen sind nur für Sie sichtbar. Nur Ihr Benutzername ist für andere auf {platform_name} sichtbar.</string>

    <!--  String for Profile Screen  -->
    <!--  Heading of Wifi Field  -->
    <string name="label_wifi">WLAN</string>
    <!--  Heading of Personal Information  -->
    <string name="label_personal_information">Persönliche informationen</string>
    <!--  Heading of Help Field  -->
    <string name="label_help">Hilfe</string>
