<resources>
  <!--An error occurred but we don't know why-->
  <string name="error_unknown">Bilinmeyen hata. Lütfen daha sonra tekrar deneyin.</string>
  <!--Course related error messages-->
  <!--Error shown when a course's content is invalid or not in the format that we expect-->
  <string name="course_error_content_invalid">Ders içeriği yükleme başarısız. Lütfen daha sonra tekrar deneyin.</string>
