    <string name="login_title">Sign in</string>
    <!-- Hint text for username/email field -->
    <string name="email_username">Username or email</string>
    <!-- Button text allowing user to reset their password -->
    <string name="forgot_password">Forgot password</string>
    <!-- Label sign in with google for social login buttons -->
    <string name="sign_in_with_google">Sign in with Google</string>
