    <string name="label_access_expires">Access expires {date}</string>
    <!-- Label for date when course expired in last 7 days like 2 days ago or 4 hours ago -->
    <string name="label_access_expired">Access expired {date}</string>
    <!-- Label for date when course expired on a specific date like on November 5 -->
    <string name="label_expired_on">Expired on {date}</string>

