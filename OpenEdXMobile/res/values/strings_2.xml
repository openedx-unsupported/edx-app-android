    <!-- Label for date when course starts -->
    <string name="label_starts">Starts {date}</string>
    <!-- Label for course that has ended -->
    <string name="label_ended_on">Ended on {date}</string>
    <!-- Label for date when course is ending -->
    <string name="label_ends">Ends {date}</string>
