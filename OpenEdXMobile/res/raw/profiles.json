{
  "fields": [
    {
      "name": "account_privacy",
      "label": "Other learners can see my:",
      "type": "switch",
      "editable": true,
      "data_type": "boolean",
      "options": {
        "values": [
          {
            "name": "Full Profile",
            "value": "all_users"
          },
          {
            "name": "Limited Profile",
            "value": "private"
          }
        ]
      },
      "instructions": "A limited profile only shares your username and profile photo.",
      "accessibility_hint": "Change what information is shared with others."
    },
    {
      "name": "year_of_birth",
      "label": "Birth year",
      "type": "select",
      "editable": true,
      "options": {
        "range_min": 1896,
        "range_max": 2020,
        "allows_none": true,
        "none_label": "No birth year"
      },
      "instructions": "You must specify your birth year before you can share your full profile.",
      "sub_instructions": "This will not be displayed publicly"
    },
    {
      "name": "country",
      "label": "Location",
      "type": "select",
      "editable": true,
      "data_type": "country",
      "options": {
        "reference": "countries",
        "allows_none": true,
        "none_label": "No location"
      }
    },
    {
      "name": "language_proficiencies",
      "label": "Primary language",
      "type": "select",
      "editable": true,
      "data_type": "language",
      "options": {
        "reference": "languages",
        "allows_none": true,
        "none_label": "No primary language"
      }
    },
    {
      "name": "bio",
      "label": "About me",
      "type": "textarea",
      "editable": true,
      "placeholder": "I would like to learn..."
    },
    {
      "name": "bio",
      "label": "User Type",
      "type": "textarea",
      "editable": false,
      "placeholder": "volunteer"
    },
    {
      "name": "bio",
      "label": "Education board",
      "type": "textarea",
      "editable": false,
      "placeholder": "ICSE"
    },
    {
      "name": "bio",
      "label": "School",
      "type": "textarea",
      "editable": false,
      "placeholder": "Children's Academy"
    },
    {
      "name": "bio",
      "label": "Grade",
      "type": "textarea",
      "editable": false,
      "placeholder": "9"
    }
  ]
}
