    <string name="settings_wifi_bottom">Télécharger les données uniquement lorsque le Wi-Fi est activé</string>
    <!-- Text to clarify that around what is visible -->
    <string name="profile_visibility_off_message">Les informations sur votre profil ne sont visibles que par vous. Seul votre nom d\'utilisateur est visible par les autres sur {platform_name}.</string>

    <!--  String for Profile Screen  -->
    <!--  Heading of Wifi Field  -->
    <string name="label_wifi">Wi-Fi</string>
    <!--  Heading of Personal Information  -->
    <string name="label_personal_information">Informations personnelles</string>
    <!--  Heading of Help Field  -->
    <string name="label_help">Aide</string>
